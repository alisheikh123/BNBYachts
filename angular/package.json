{
  "name": "bnb-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod=true --outputPath=dist --outputHashing=all --deleteOutputPath=true",
    "stag": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=staging --prod=true --outputPath=dist --outputHashing=all --deleteOutputPath=true",
    "hmr": "ng serve --host 0.0.0.0 --port 4200 --hmr",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "lint": "tslint --force --project src/tsconfig.json src/**/*.ts -t verbose"
  },
  "private": true,
